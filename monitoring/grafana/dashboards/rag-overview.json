{
  "uid": "rag-overview",
  "title": "RAG Overview",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "tags": ["rag", "prometheus"],
  "time": { "from": "now-1h", "to": "now" },
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "Retrieval RPS",
      "datasource": "Prometheus",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(rag_requests_total[1m])) by (status)",
          "legendFormat": "{{status}}"
        }
      ]
    },
    {
      "id": 10,
      "type": "timeseries",
      "title": "rag-gate RPS",
      "datasource": "Prometheus",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(gate_requests_total[1m])) by (status)",
          "legendFormat": "{{status}}"
        }
      ]
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "Retrieval p95 stage latency (s)",
      "datasource": "Prometheus",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum(rate(rag_stage_latency_seconds_bucket[5m])) by (le, stage))",
          "legendFormat": "{{stage}}"
        }
      ]
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "Gate RPS",
      "datasource": "Prometheus",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(gate_requests_total[1m])) by (status)",
          "legendFormat": "{{status}}"
        }
      ]
    },
    {
      "id": 4,
      "type": "timeseries",
      "title": "Gate p95 stage latency (s)",
      "datasource": "Prometheus",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum(rate(gate_latency_seconds_bucket[5m])) by (le, stage))",
          "legendFormat": "{{stage}}"
        }
      ]
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "Storage errors rate",
      "datasource": "Prometheus",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(storage_errors_total[5m])) by (stage, kind)",
          "legendFormat": "{{stage}}/{{kind}}"
        }
      ]
    },
    {
      "id": 6,
      "type": "timeseries",
      "title": "Prometheus up",
      "datasource": "Prometheus",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 16 },
      "targets": [
        {
          "refId": "A",
          "expr": "up",
          "legendFormat": "{{job}}"
        }
      ]
    }
  ]
}


