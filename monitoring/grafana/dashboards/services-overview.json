{
  "uid": "services-overview",
  "title": "Services Overview",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "tags": ["rag", "services", "prometheus"],
  "time": { "from": "now-1h", "to": "now" },
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "rag-gate RPS (by status)",
      "datasource": "Prometheus",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "targets": [
        { "refId": "A", "expr": "sum(rate(gate_requests_total[1m])) by (status)", "legendFormat": "{{status}}" }
      ]
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "retrieval RPS (by status)",
      "datasource": "Prometheus",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "targets": [
        { "refId": "A", "expr": "sum(rate(rag_requests_total[1m])) by (status)", "legendFormat": "{{status}}" }
      ]
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "doc-processor RPS (by status)",
      "datasource": "Prometheus",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "targets": [
        { "refId": "A", "expr": "sum(rate(processor_requests_total[1m])) by (status)", "legendFormat": "{{status}}" }
      ]
    },
    {
      "id": 4,
      "type": "timeseries",
      "title": "document-storage RPS (by status)",
      "datasource": "Prometheus",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "targets": [
        { "refId": "A", "expr": "sum(rate(storage_requests_total[1m])) by (status)", "legendFormat": "{{status}}" }
      ]
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "rag-gate latency p95 (by stage)",
      "datasource": "Prometheus",
      "gridPos": { "h": 10, "w": 12, "x": 0, "y": 16 },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum(rate(gate_latency_seconds_bucket[5m])) by (le, stage))",
          "legendFormat": "{{stage}}"
        }
      ]
    },
    {
      "id": 6,
      "type": "timeseries",
      "title": "retrieval latency p95 (by stage)",
      "datasource": "Prometheus",
      "gridPos": { "h": 10, "w": 12, "x": 12, "y": 16 },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum(rate(rag_stage_latency_seconds_bucket[5m])) by (le, stage))",
          "legendFormat": "{{stage}}"
        }
      ]
    },
    {
      "id": 7,
      "type": "timeseries",
      "title": "doc-processor latency p95 (by stage)",
      "datasource": "Prometheus",
      "gridPos": { "h": 10, "w": 12, "x": 0, "y": 26 },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum(rate(processor_latency_seconds_bucket[5m])) by (le, stage))",
          "legendFormat": "{{stage}}"
        }
      ]
    },
    {
      "id": 8,
      "type": "timeseries",
      "title": "document-storage latency p95 (by stage)",
      "datasource": "Prometheus",
      "gridPos": { "h": 10, "w": 12, "x": 12, "y": 26 },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum(rate(storage_latency_seconds_bucket[5m])) by (le, stage))",
          "legendFormat": "{{stage}}"
        }
      ]
    }
  ]
}



